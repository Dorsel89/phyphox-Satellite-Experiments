<phyphox xmlns="http://phyphox.org/xml" xmlns:editor="http://phyphox.org/editor/xml" version="1.9" editor:version="1.0" locale="en">
    <title>Live Mode</title>
    <category>phyphox electricity box</category>
    <color>ffbe1a</color>
    <description>Measuring and plotting the voltages on both Channels 1 and 2. It is also an option to measure the current on Channel 1 via voltage drop across a chosen resistance.</description>
    <icon format="base64"></icon>

	<translations>
		<translation locale="de">
			<title>Live Modus</title>
			<category>phyphox E-Lehre-Box</category>
			<description>Zeigt die an den Kanälen anliegende Spannung.</description>
			<string original="Rawdata">Rohdaten</string>
			<string original="resistor for current measurement">Ohmscher Widerstand zur Strommessung:</string>
			<string original="voltage">Spannung</string>
			<string original="voltage CH1">Spannung Kanal 1</string>
			<string original="voltage CH2">Spannung Kanal 2</string>
			<string original="current">Strom</string>
			<string original="average current">durchschnittlicher Strom</string>
			<string original="time">Zeit</string>
			<string original="U-I-plot">U-I-Diagramm</string>
			<string original="voltage CH1">Spannung Kanal 1</string>
			<string original="current CH2">Strom Kanal 2</string> "I-U-plot"
			<string original="voltages CH1 and CH2">Spannung Kana 1 & 2</string> "I-U-plot"
			
		</translation>
	</translations>
	
	<data-containers>
        <container size="0" static="false">voltageCH1</container>
        <container size="0" static="false">voltageCH2</container>
        <container size="0" static="false">timeCH1</container>
		<container size="0" static="false">timeCH2</container>
		<container size="1" static="false">timeCH1_Offset</container>
		<container size="1" static="false">timeCH2_Offset</container>
		<container size="0" static="false">timeCH1_new</container>
		<container size="0" static="false">timeCH2_new</container>
		<container size="1" static="false">resistorCH1</container>
		<container size="0" static="false">currentCH1</container>
		<container size="0" static="false">IaverageCH1</container>
    </data-containers>
    <input>
        <bluetooth editor:uuid="101"  name="E-Lehre"  mode="notification" rate="1" subscribeOnStart="false">
            <config char="cddf1003-30f7-4671-8b43-5e40ba53514a" conversion="hexadecimal">0C80</config>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian">voltageCH1</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="4">voltageCH2</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="8">timeCH1</output>
            <output char="cddf1002-30f7-4671-8b43-5e40ba53514a" conversion="float32LittleEndian" offset="12">timeCH2</output>
        </bluetooth>
    </input>
    <output>
    </output>
    <analysis sleep="0"  onUserInput="false">
		<first>
			<input clear="false">timeCH1</input>
			<output>timeCH1_Offset</output>
		</first>
		
		<subtract>
			<input clear="false">timeCH1</input>
			<input clear="false">timeCH1_Offset</input>
			<output>timeCH1_new</output>
		</subtract>
		
		<first>
			<input clear="false">timeCH2</input>
			<output>timeCH2_Offset</output>
		</first>
		
		<subtract>
			<input clear="false">timeCH2</input>
			<input clear="false">timeCH2_Offset</input>
			<output>timeCH2_new</output>
		</subtract>
		
        <divide>
            <input as="dividend" clear="false">voltageCH1</input>
            <input as="divisor" clear="false">resistorCH1</input>
            <input as="divisor" type="value">0.001</input>
            <output as="quotient">currentCH1</output>
        </divide>
		
        <average>
            <input as="buffer" clear="false">currentCH1</input>
            <output as="average">IaverageCH1</output>
        </average>
		
    </analysis>
    <views>
        <view label="voltages CH1 and CH2">
            <graph label="t-U-plot CH1" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="time" labelY="voltage" labelZ="" unitX="s" unitY="V" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0"  mapWidth="0"       >
                <input axis="x">timeCH1_new</input>
                <input axis="y">voltageCH1</input>
            </graph>
            <value label="voltage CH1" size="1" precision="3" scientific="false" unit="V" factor="1" >
                <input>voltageCH1</input>
            </value>
            <graph label="t-U-plot CH2" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="time" labelY="voltage" labelZ="" unitX="s" unitY="V" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0"  mapWidth="0"       >
                <input axis="x">timeCH2_new</input>
                <input axis="y">voltageCH2</input>
            </graph>
            <value label="voltage CH2" size="1" precision="3" scientific="false" unit="V" factor="1" >
                <input>voltageCH2</input>
            </value>
        </view>
	<view label="ammeter CH1">
            <edit label="resistor for current measurement" signed="false" decimal="false" unit="Ω" factor="1" default="10">
                <output>resistorCH1</output>
            </edit>
            <graph editor:uuid="159" editor:posx="1225.84375" editor:posy="483.71875" label="t-I-plot CH1" aspectRatio="2.5" style="lines" lineWidth="1" color="ff7e22" partialUpdate="false" history="1" labelX="time" labelY="current" labelZ="" unitX="s" unitY="mA" unitZ="" logX="false" logY="false" logZ="false" xPrecision="3" yPrecision="3" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0"  mapWidth="0"       >
                <input axis="x">timeCH1_new</input>
                <input axis="y">currentCH1</input>
            </graph>
            <value editor:uuid="160" editor:posx="1218.984375" editor:posy="660.03125" label="current CH1" size="1" precision="3" scientific="false" unit="mA" factor="1" >
                <input>currentCH1</input>
            </value>
            <value editor:uuid="161" editor:posx="1512.609375" editor:posy="829.015625" label="average current" size="1" precision="3" scientific="false" unit="mA" factor="1" >
                <input>IaverageCH1</input>
            </value>
        </view>
    </views>
    <export>
	<set name="Rawdata">
	    <data name="Time CH1 (s)">timeCH1_new</data>
        <data name="Voltage CH1 (V)">voltageCH1</data>
	    <data name="Time CH2 (s)">timeCH2_new</data>
        <data name="Voltage CH2 (V)">voltageCH2</data>
        <data name="Current CH1 (mA)">currentCH1</data>
     </set>
    </export>
</phyphox>